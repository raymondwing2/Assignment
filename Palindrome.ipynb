   "source": [
    "def isPalindrome(s):\n",
    "    return s.lower() == s[::-1].lower()\n",
    " \n",
    "#Main\n",
    "\n",
    "s = input(\"Enter a word: \")\n",
    "ans = isPalindrome(s)\n",
    " \n",
    "if ans:\n",
    "    print(\"Yes, {} is palindrome\".format(s))\n",
    "else:\n",
    "    print(\"No, {} is NOT palindrome\".format(s))"
